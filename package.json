{
  "name": "untapped-wordpress-widget",
  "version": "1.4.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "start": "wp-scripts start",
    "dev": "wp-scripts start",
    "watch": "wp-scripts start",
    "build": "wp-scripts build",
    "lint:css": "wp-scripts lint-style && ./node_modules/.bin/sass-lint --config ./node_modules/@webdevstudios/css-coding-standards/.sass-lint.yml '**/*.scss' --verbose",
    "lint:js": "wp-scripts lint-js",
    "lint:php": "./vendor/bin/phpcs --standard=./.phpcs.xml.dist --extensions=php ./ --ignore='./build/*,./node_modules/*,./vendor/*'",
    "packages-update": "wp-scripts packages-update",
    "postinstall": "wp-scripts build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tw2113/Untappd-WordPress-Widget.git"
  },
  "author": "Michael Beckwith",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/tw2113/Untappd-WordPress-Widget/issues"
  },
  "homepage": "",
  "devDependencies": {
    "@webdevstudios/css-coding-standards": "^1.0.1",
    "@webdevstudios/js-coding-standards": "^1.3.0",
    "@wordpress/scripts": "^22.3.0",
    "acorn": "^8.7.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^5.1.5",
    "ignore-emit-webpack-plugin": "^2.0.6",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "postcss-safe-parser": "^6.0.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@wordpress/eslint-plugin/esnext",
      "plugin:@wordpress/eslint-plugin/react",
      "@webdevstudios/js-coding-standards"
    ]
  },
  "sasslintConfig": "./node_modules/@webdevstudios/css-coding-standards/.sass-lint.yml",
  "stylelint": {
    "extends": "stylelint-config-wordpress",
    "ignoreFiles": [
      "**/*.scss",
      "build/**/*.css"
    ]
  },
  "dependencies": {}
}
